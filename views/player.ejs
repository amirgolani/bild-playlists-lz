<!DOCTYPE html>
<html lang="en">

<head>
  <title>Player</title>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- <link rel="stylesheet" href="/assets/css/style_11Themen.css"> -->
  <link rel="stylesheet" href="/assets/css/player.css">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.3/gsap.min.js"></script>
  <script src="https://unpkg.com/gsap@3.9.0/dist/Draggable.min.js"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/brands.min.css" integrity="sha512-8RxmFOVaKQe/xtg6lbscU9DU0IRhURWEuiI0tXevv+lXbAHfkpamD4VKFQRto9WgfOJDwOZ74c/s9Yesv3VvIQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="apple-touch-icon" sizes="180x180" href="/assets/gp/favicon-play/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/assets/gp/favicon-play/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/assets/gp/favicon-play/favicon-16x16.png">
  <link rel="manifest" href="/assets/gp/favicon-play/site.webmanifest">
</head>

<body id="endPoint" name="<%= playlist %>" style="position: absolute; overflow: clip; transform-origin: top left; margin: 0%; padding: 0%; background-color: black; width: <%= width %>; height: <%= height %>; scale: <%= size %>;">
  <div class="full-hd-container">
    <div class="full-hd-container" id="videoPlayer">
      <div class="full-hd-container">
        <video preload="auto" style="opacity: .4 ; position: absolute; scale: 4; filter: blur(10px);" width="1920" height="1080" id="video-bg" playsinline autoplay muted loop>
          <source src="" type="video/webm">
        </video>
      </div>
      <video preload="auto" style="opacity: 0; position: absolute; box-shadow: 0px 0px 50px 50px #000000;" width="1920" height="1080" id="video" playsinline autoplay muted loop>
        <source src="<%= bg %>" type="<%= vidType %>">
      </video>
    </div>

    <div class="full-hd-container" id="itemTitles">
      <div id="mainTitle" class="main-title" style="background-image: url(<%= headline %>);"></div>
      <div id="mainTitle" class="main-title" style="background-image: url(<%= headline %>);"></div>
    </div>
    <div class="full-hd-container" id="imageDisplay" hidden>
      <div id="backImage" class="full-hd-container" style="background-image: url(); background-position: center; background-size: cover; scale: 3; filter: blur(50px); opacity: 0;">
      </div>
      <div id="frontImage" class="full-hd-container" style="scale: .75; opacity: 0; display: flex; justify-content: center; align-items: center;">
        <img id="mainImage" src="" alt="" style="height: 100%;">
      </div>
    </div>

    <div class="full-hd-container" id="webDisplay" hidden>
      <iframe id="webIframe" src="/" width="1920" height="1080" frameBorder="0" hidden></iframe>
    </div>

    <div id="bottom-dark-blur" class="bottom-dark-blur"></div>
    <div id="left-dark-blur" class="left-dark-blur" style="visibility: hidden;"></div>

    <!-- <div class="hp-img" id="imageContainer"></div> -->
    <div id="layoutBuildUp">
      <div id="theMenu" class="menu-container">
        <div onclick="setImage(''); setUrl(''); handleTitles('<%= icon %>', '<%= title %>', '<%= sub %>'); handleSelect(this.id); playVideo('<%= bg %>', 'loop', 'muted', 'noPlayButtons', '1', '0.04');" id="b_0" class="card-in-menu" style="width: 360px;
                background-image: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgb(0,0,0,1) 100%);">
          <div id="listTitles" class="title-in-menu">
            <div style="background-image: url(/assets/gp/ICON_02_home.png);
            background-size: cover;
            width: 26px;
            height: 26px;
            opacity: .6"></div>
          </div>
        </div>
      </div>
    </div>
    <canvas class="full-hd-container" id="paintCanvas" width="1920" height="1080" hidden></canvas>
    <div id="videoTitleToggle" style="opacity: 0; position: relative; left: 200px">
      <div hidden id="playing-icon" class="video-icon">
      </div>
      <div hidden id="playing-title" class="video-title">
        <%= title %>
      </div>
      <div hidden id="playing-timestamp" class="video-timestamp">
        <%= sub %>
      </div>
    </div>
    <div id="playlists-icon" class="playlists-icon" onclick="window.location.assign('/playlists')"></div>

    <div id="menuToggle" style="position: absolute; opacity: 0; ">
      <div id="seek" style="opacity: 0;">
        <div id="play-icon" class="pause-icon" onclick="handlePlays()"></div>
        <input id="seekslider" class="seekslider" type="range" min="0" max="1500" value="0" step="1">
        <!-- <div id="timecode" class="timecode"> -->
          <!-- 00:00</div> -->
      </div>
      <div hidden id="zoom-icon" class="zoom-icon" onclick="handleZoom()"></div>
      <div id="menuPosIcon" class="menuPos-icon" onclick="handleMenuPos()"></div>
      <div id="home-icon" class="home-icon" onclick="handleScrollPos()"></div>
      <div id="art-icon" class="art-icon" onclick="manageDraws()"></div>
      <div hidden id="blue-circle" class="blue-circle" onclick="manageColors('#004f98')"></div>
      <div hidden id="red-circle" class="red-circle" onclick="manageColors('#ee2e31')"></div>
      <div hidden id="black-circle" class="black-circle" onclick="manageColors('#080808')"></div>
      <div hidden id="white-circle" class="white-circle" onclick="manageColors('#f8f8ff')"></div>
    </div>

  </div>

  <script src="/assets/js/player.js"></script>

</body>

</html>